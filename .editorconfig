root = true

# Unix-style newlines with a newline ending every file
[*]
end_of_line = lf
insert_final_newline = true

[*.java]
charset = utf-8
indent_style = space
intent_size = 2
trim_trailing_whitespace = true

[*.xml]
charset = utf-8
indent_style = space
intent_size = 2
trim_trailing_whitespace = true
